{
    "mcpServers": {
      "filesystem": {
        "command": "npx",
        "args": [
          "-y",
          "@modelcontextprotocol/server-filesystem",
          "/Users/granludo/Desktop",
          "/Users/granludo/Downloads"
        ]
      },
      "desktop-commander": {
        "command": "npx",
        "args": ["@wonderwhy-er/desktop-commander@latest"]
      },
      "playwright": {
        "command": "npx",
        "args": ["@playwright/mcp@latest"]
      },
      "search-images": {
        "command": "/opt/homebrew/Caskroom/mambaforge/base/bin/uv",
        "args": [
          "--directory",
          "/opt/agents/google-image-search-mcp-python",
          "run",
          "main.py"
        ]
      },
      "research-stdio": {
        "command": "/opt/homebrew/Caskroom/mambaforge/base/bin/uv",
        "args": [
          "--directory",
          "/opt/agents/research-server",
          "run",
          "research_server.py"
        ]
      },
      "remote-research-server": {
        "command": "npx",
        "args": ["mcp-remote", "http://localhost:8001/sse", "--allow-http"]
      },
      "research-scholar": {
        "command": "/opt/homebrew/Caskroom/mambaforge/base/bin/uv",
        "args": [
          "--directory",
          "/opt/agents/mcp-research-server-s",
          "run",
          "research_server_multi.py"
        ]
      },
      "research-scholar-venv": {
        "command": "/bin/bash",
        "args": [
          "-c",
          "cd /opt/agents/mcp-research-server-s && source .venv/bin/activate && python main.py"
        ]
      },
      "research-scholar-venv-verbose": {
        "command": "/bin/bash",
        "args": [
          "-c",
          "cd /opt/agents/mcp-research-server-s && source .venv/bin/activate && python main.py --verbose"
        ]
      }

    }
}